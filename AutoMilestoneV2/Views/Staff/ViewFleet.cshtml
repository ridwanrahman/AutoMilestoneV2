
@{
    ViewBag.Title = "ViewFleet";
    Layout = "~/Views/Shared/StaffLayout.cshtml";
}

<h2>ViewFleet</h2>
<body>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Model</th>
                <th scope="col">Image</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var data in ViewBag.ItemData)
            {
                <tr>
                    <th scope="row"></th>
                    <td>@data.Name</td>
                    <td>@data.Model</td>
                    <td>mdo</td>
                </tr>
            }
        </tbody>

</body>

